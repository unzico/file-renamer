{
  "name": "simple-file-renamer",
  "version": "0.5.1",
  "description": "Build your own file renamer.",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "nodemon",
    "prebuild": "rm -rf dist/",
    "build": "yarn run build:tsc && yarn run build:compile",
    "build:tsc": "tsc",
    "build:compile": "babel ./src -d ./dist -s --extensions \".js,.ts\" --ignore \"__tests__/*,__mocks__/*\"",
    "release": "np --no-2fa",
    "upgrade-deps": "yarn upgrade-interactive --latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unzico/simple-file-renamer"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/node": "7.14.2",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.14.2",
    "@babel/plugin-proposal-object-rest-spread": "7.14.4",
    "@babel/plugin-proposal-optional-chaining": "7.14.2",
    "@babel/preset-env": "7.14.4",
    "@babel/preset-typescript": "7.13.0",
    "@babel/register": "7.13.16",
    "@types/inquirer": "7.3.1",
    "@types/node": "15.12.1",
    "babel-plugin-module-resolver": "4.1.0",
    "nodemon": "2.0.7",
    "np": "7.5.0",
    "prettier": "2.3.0",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.3.2"
  },
  "dependencies": {
    "kleur": "^4.1.4"
  }
}
